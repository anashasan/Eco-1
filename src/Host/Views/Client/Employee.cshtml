@model Host.DataModel.EmployeeProfileDto
@{
    Layout = "_ClientDashboard";
}
<section class="main-contents">
    <div class="row">
        <div class="col-sm-12">
            <div class="panel panel-default table-panel">
                <div class="panel-title">

                    <div class="panel-controls">

                    </div>
                    <h3 class="panel-heading">
                        Basic information<span class="badge" style="background-color: #6765da; margin-left: 2em"></span>
                        <a class="btn btn-xs btn-default" href="@Url.Action("EditEmployee","Client")">Edit</a>

                    </h3>

                </div>
                <div class="panel-body no-padding">
                    <div class="table-responsive">
                        <input data-val="true" data-val-required="The EmployeeProfileId field is required." id="EmployeeProfileId" name="EmployeeProfileId" type="hidden" value="b7051ad3-a242-48b4-9857-105cdde9a695">
                        <table class="table table-striped table-bordered sm-bottom0">
                            <tbody>
                                <tr>
                                    <td style="width: 120px;">First Name</td>
                                    <td>@Model.FirstName</td>
                                    <td style="width: 140px;">Last Name</td>
                                    <td>@Model.LastName</td>
                                </tr>
                                <tr>
                                    <td>Address</td>
                                    <td colspan="3">@Model.StreetAddress</td>
                                </tr>
                                <tr>
                                    <td>Gender</td>
                                    <td></td>
                                    <td>City</td>
                                    <td>@Model.City</td>
                                </tr>
                                <tr>
                                    <td>Date Of Birth</td>
                                    <td colspan="3">@Model.DateOfBirth</td>
                                </tr>
                                <tr>
                                    <td>Phone No</td>
                                    <td>@Model.CellPhone</td>
                                </tr>


                                <tr>
                                    <td>Work Email</td>
                                    <td colspan="3">@Model.WorkEmail</td>
                                </tr>
                                <tr>
                                    <td>Job Title</td>
                                    <td></td>
                                    <td>Zip Code</td>
                                    <td></td>
                                </tr>


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

